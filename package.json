{
  "name": "ts-micro-result",
  "version": "2.1.1",
  "description": "Result toolkit for TypeScript services, SDKs, and background jobs",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core/*": {
      "import": "./dist/core/*.js",
      "types": "./dist/core/*.d.ts"
    },
    "./factories/*": {
      "import": "./dist/factories/*.js",
      "types": "./dist/factories/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run minify",
    "minify": "terser dist/index.js dist/core/result.js dist/core/types.js dist/factories/ok.js dist/factories/err.js dist/factories/errors.js dist/factories/validation.js dist/utils/guards.js dist/utils/http.js dist/utils/serialization.js --compress --mangle --module -d dist",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "typescript",
    "result",
    "error-handling",
    "functional-programming",
    "either",
    "monad"
  ],
  "author": "minhtaimc",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^5.0.0",
    "terser": "^5.44.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minhtaimc/ts-micro-result.git"
  },
  "bugs": {
    "url": "https://github.com/minhtaimc/ts-micro-result/issues"
  },
  "homepage": "https://github.com/minhtaimc/ts-micro-result#readme"
}
