{
  "name": "ts-micro-result",
  "version": "2.1.10",
  "description": "Result toolkit for TypeScript services, SDKs, and background jobs",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lite": {
      "import": "./dist/lite.js",
      "types": "./dist/lite.d.ts"
    },
    "./core/*": {
      "import": "./dist/core/*.js",
      "types": "./dist/core/*.d.ts"
    },
    "./factories/*": {
      "import": "./dist/factories/*.js",
      "types": "./dist/factories/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "result",
    "error-handling",
    "functional-programming",
    "either",
    "monad"
  ],
  "author": "minhtaimc",
  "license": "MIT",
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minhtaimc/ts-micro-result.git"
  },
  "bugs": {
    "url": "https://github.com/minhtaimc/ts-micro-result/issues"
  },
  "homepage": "https://github.com/minhtaimc/ts-micro-result#readme"
}
